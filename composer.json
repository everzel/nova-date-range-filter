{
    "name": "everzel/nova-date-range-filter",
    "description": "A Laravel Nova date range filter.",
    "keywords": [
        "laravel",
        "nova",
        "date",
        "filter"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Everzel\\Filters\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Everzel\\Filters\\FilterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}